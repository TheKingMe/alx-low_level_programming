/**
*rev_string - reverse the string
*@s: the string
*/
void rev_string(char *s)
{
int i = 0, j = 0, n;
char *sr = s;
while (s[i] != '\0')
{
i++;
}#include "main.h"
   /**
    *rev_string - reverse the string
    *@s: the string
    */
   void rev_string(char *s)
   {
     int i = 0, j = 0, n;
     char *sr = s;
     while (s[i] != '\0')
       {
	 i++;
       }
     for (n = i - 1; n >= 0; n--)
       {
	 s[j] = sr[n];
	 j++;
       }
   }
for (n = i - 1; n >= 0; n--)
{
s[j] = sr[n];
j++;
}
}
